{
  "parser": "babel-eslint",
  "extends": ["standard", "standard-react"],
  "plugins": ["babel", "react", "promise"],
  "env": {
    "browser": true
  },
  "globals": {},
  "rules": {
    "key-spacing": 0,
    "jsx-quotes": 0,
    "max-len": [2, 120, 2],
    "object-curly-spacing": [2, "always"],
    "space-before-function-paren": 0,
    "spaced-comment": 0,
    "react/jsx-indent-props": 0,
    "react/jsx-indent": 0,
    "generator-star": 0,
    "generator-star-spacing": 0
  }
}
